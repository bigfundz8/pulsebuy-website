{
  "name": "pulsebuy-website",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "pages/api/automation/*.js": {
      "maxDuration": 300
    },
    "pages/api/cron/*.js": {
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/automation/master-controller?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/automation/auto-products-enhanced?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/automation/profit-optimization?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/automation/auto-order-forwarding?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/automation/auto-orders?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 */2 * * *"
    },
    {
      "path": "/api/automation/auto-marketing?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 12 * * *"
    },
    {
      "path": "/api/automation/full-automation?api_key=ba6b3126c48d0e15dfc8e71508c9362574fbac7b1eafce0bbba7445b7b2f3ab7",
      "schedule": "0 18 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}